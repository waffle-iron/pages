<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/vortex/eventListener.xsd">
    <import>

        <eventlistener name="userGroupAddFormDisplayCategoryAssignVariables">
            <environment>admin</environment>
            <eventclassname>wcf\acp\form\UserGroupAddForm</eventclassname>
            <eventname>assignVariables</eventname>
            <listenerclassname>wcf\system\event\listener\GroupDisplayCategoryFieldListener</listenerclassname>
            <inherit>1</inherit>
        </eventlistener>

        <eventlistener name="userGroupAddFormDisplayCategoryReadFormParameters">
            <environment>admin</environment>
            <eventclassname>wcf\acp\form\UserGroupAddForm</eventclassname>
            <eventname>readFormParameters</eventname>
            <listenerclassname>wcf\system\event\listener\GroupDisplayCategoryFieldListener</listenerclassname>
            <inherit>1</inherit>
        </eventlistener>

        <eventlistener name="userGroupAddFormDisplayCategorySave">
            <environment>admin</environment>
            <eventclassname>wcf\acp\form\UserGroupAddForm</eventclassname>
            <eventname>save</eventname>
            <listenerclassname>wcf\system\event\listener\GroupDisplayCategoryFieldListener</listenerclassname>
            <inherit>1</inherit>
        </eventlistener>

        <eventlistener name="userGroupAddFormDisplayCategoryReadData">
            <environment>admin</environment>
            <eventclassname>wcf\acp\form\UserGroupEditForm</eventclassname>
            <eventname>readData</eventname>
            <listenerclassname>wcf\system\event\listener\GroupDisplayCategoryFieldListener</listenerclassname>
        </eventlistener>

        <eventlistener name="userRegistrationComplete">
            <environment>user</environment>
            <eventname>finalizeAction</eventname>
            <eventclassname>wcf\data\AbstractDatabaseObjectAction</eventclassname>
            <listenerclassname>wcf\system\event\listener\UserRegistrationCompleteListener</listenerclassname>
        </eventlistener>

    </import>
</data>